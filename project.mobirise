    "archive.html": {
      "settings": {
        "meta_descr": "Testimonial archive for Fairwind Yacht Charters",
        "title": "Testimonial archive",
        "order": 15
      },
      "components": [
        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
